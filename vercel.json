{
  "functions": {
    "api/send-email.js": {
      "maxDuration": 10
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- dashboard.html dashboard-customer-management.html data/ google-apps-script/",
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}